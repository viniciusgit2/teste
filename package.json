  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "builddev": "webpack --mode=development",
    "buildpro": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "build2": "./node_modules/.bin/babel --out-dir lib src",
    "buildmusic1": "./node_modules/.bin/webpack --output-filename  music.umd.js --mode=production ",
    "buildmusic2": "./node_modules/.bin/webpack --output-filename  music.umd.min.js --mode=production ",
    "buildall": " yarn build2  &&  yarn buildmusic1 && yarn buildmusic2",
    "webservedev": "./node_modules/.bin/webpack server --mode=development ",
    "clear": "rimraf lib && rimraf node_modules"
  },
